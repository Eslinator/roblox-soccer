{
  "dry_run": false,
  "steps": [
    { "type": "run", "cmd": "which stylua || true" },
    { "type": "run", "cmd": "which selene || true" },
    { "type": "run", "cmd": "ls -l ~/.aftman/bin/stylua ~/.aftman/bin/selene || true" },
    { "type": "run", "cmd": "env AFTMAN_LOG=trace aftman install || true" },
    { "type": "run", "cmd": "stylua --version || true" },
    { "type": "run", "cmd": "selene --version || true" }
  ]
}
